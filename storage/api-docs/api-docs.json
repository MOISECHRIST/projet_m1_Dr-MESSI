        "/publications/text-with-images": {
            "post": {
                "tags": [
                    "Publications"
                ],
                "summary": "Créer une publication de type texte avec images",
                "description": "Crée une publication contenant du texte et une ou plusieurs images. Les images doivent être au format JPEG, PNG, JPG ou GIF, avec une taille maximale de 2 Mo par image.",
                "operationId": "cbba4a9e3dfb5ceca1757810b40db5d3",
                "requestBody": {
                    "description": "Données nécessaires pour créer une publication texte + images",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "user_id",
                                    "content",
                                    "images",
                                    "title"
                                ],
                                "properties": {
                                    "user_id": {
                                        "description": "ID de l'utilisateur créant la publication",
                                        "type": "integer",
                                        "example": 1
                                    },
                                    "content": {
                                        "description": "Contenu principal de la publication",
                                        "type": "string",
                                        "example": "Contenu textuel de la publication"
                                    },
                                    "images": {
                                        "description": "Liste des images à associer à la publication",
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary"
                                        }
                                    },
                                    "title": {
                                        "description": "Titre de la publication",
                                        "type": "string",
                                        "example": "Titre de la publication"
                                    },
                                    "description": {
                                        "description": "Description optionnelle de la publication",
                                        "type": "string",
                                        "example": "Description de la publication"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Publication texte + images créée avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Publication créée avec succès"
                                        },
                                        "publication": {
                                            "$ref": "#/components/schemas/Publication"
                                        },
                                        "images": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/PublicationImage"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Erreur de validation ou autre erreur",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Une erreur s'est produite lors de la création de la publication."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur serveur interne",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Erreur serveur interne"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
